class Solution {
    public int searchInsert(int[] nums, int target) {
        int start=0,end=nums.length-1;

        while(start<=end){
            int mid=start+(end-start)/2;

            if(nums[mid]==target){
                return mid;
            }
            if(target>nums[mid]){
                start=start+1;

            }
            else{
                end=end-1;
            }
        }
        return start;


    }
}
